{
  "name": "timehub",
  "version": "0.4.0",
  "description": "A web app to browse the story of your repo",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uesteibar/timehub.git"
  },
  "author": "Unai Esteibar <uesteibar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uesteibar/timehub/issues"
  },
  "homepage": "https://github.com/uesteibar/timehub",
  "scripts": {
    "test": "",
    "start": "node app",
    "postinstall": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "prettier": "prettier --trailing-comma all --single-quote --write --jsx-bracket-same-line --bracket-spacing  'frontend/**/*.js'",
    "lint:staged": "lint-staged",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "bump": "standard-version",
    "deploy": "npm run bump && git push heroku master && git push --follow-tags origin master"
  },
  "lint-staged": {
    "frontend/**/*.js": [
      "npm run prettier",
      "npm run eslint:fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "apicache": "^0.8.4",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.22.1",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "jss": "^7.1.1",
    "moment": "^2.18.1",
    "octicons": "^5.0.1",
    "prop-types": "^15.5.8",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-event-timeline": "^1.2.2",
    "react-jss": "^6.1.1",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "webpack": "^1.12.2",
    "html-webpack-plugin": "^1.6.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "stats-webpack-plugin": "^0.2.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "json-loader": "^0.5.3",
    "css-loader": "^0.19.0",
    "rimraf": "^2.4.3",
    "request-promise": "^4.2.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "cross-env": "^1.0.7",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "jsdom": "^6.5.1",
    "lint-staged": "^3.4.0",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.2.2",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "standard-version": "^4.0.0",
    "style-loader": "^0.12.4"
  }
}
